// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model book {
  id          Int      @id @default(autoincrement())
  title       String
  author_name String
  description String
  groups      String
}